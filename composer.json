{
    "name": "ccharz/laravel-dto-lite",
    "description": "Simple Laravel Data Transfer Object",
    "keywords": [
        "laravel",
        "dto"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christian Charzewski",
            "email": "chris@charzewski.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^8.0||^9.0||^10.0",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ccharz\\DtoLite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccharz\\DtoLite\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit --coverage-html coverage --coverage-text"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ccharz\\DtoLite\\LaravelDtoLiteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}